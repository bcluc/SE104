        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.SE104"
        tools:targetApi="31">
        <activity
            android:name=".navigation_bottom"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"></activity>
        <activity
            android:name=".fragment_home"
            android:exported="true"></activity>
        <activity
            android:name=".activity_message"
            android:exported="true"></activity>
        <activity android:label="SE104" android:name=".choose_trip"> </activity>
        <activity android:label="SE104" android:name=".check_out"> </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>